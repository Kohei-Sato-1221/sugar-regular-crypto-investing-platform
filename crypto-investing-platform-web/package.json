{
	"name": "crypto-investing-platform-web",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "prisma migrate dev --schema=../db/prisma/schema.prisma",
		"db:migrate": "prisma migrate deploy --schema=../db/prisma/schema.prisma",
		"db:push": "prisma db push --schema=../db/prisma/schema.prisma",
		"db:studio": "prisma studio --schema=../db/prisma/schema.prisma",
		"dev": "next dev --turbo",
		"postinstall": "prisma generate --schema=../db/prisma/schema.prisma",
		"preview": "next build && next start",
		"start": "next start",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:fe": "vitest run src/app --passWithNoTests",
		"test:be": "vitest run src/server",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.7.2",
		"@aws-sdk/client-cognito-identity-provider": "^3.958.0",
		"@prisma/client": "^6.6.0",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"next": "^15.2.3",
		"next-auth": "5.0.0-beta.25",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"server-only": "^0.0.1",
		"superjson": "^2.2.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@tailwindcss/postcss": "^4.0.15",
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.1",
		"@types/node": "^20.14.10",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react": "^4.2.1",
		"jsdom": "^23.0.1",
		"postcss": "^8.5.3",
		"prisma": "^6.6.0",
		"tailwindcss": "^4.0.15",
		"typescript": "^5.8.2",
		"vite": "^5.1.0",
		"vitest": "^1.2.0"
	},
	"ct3aMetadata": {
		"initVersion": "7.40.0"
	}
}
